    "db:migrate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "run:dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify ./src/common/infra/http/server.ts",
    "run:prod": "node -r tsconfig-paths/register dist/common/infra/http/server.js"
  },
